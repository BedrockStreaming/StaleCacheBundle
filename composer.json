{
  "name": "bedrock/stale-cache-bundle",
  "description": "Add a stale mode into Symfony cache",
  "type": "symfony-bundle",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "Bedrock\\StaleCacheBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bedrock\\StaleCacheBundle\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Valentin Clavreul",
      "email": "valentin.clavreul@bedrockstreaming.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "symfony/dependency-injection": "^4.4 || ^5.4",
    "symfony/http-kernel": "^4.4 || ^5.4",
    "symfony/cache-contracts": "^1.0 || ^2.0",
    "symfony/cache": "^4.4 || ^5.4",
    "symfony/config": "^4.4 || ^5.4"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.4 || ^5.4",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0"
  }
}
